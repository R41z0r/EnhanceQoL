package-as: EnhanceQoL

externals:
  EnhanceQoL/libs/LibDBIcon-1.0:
    url: https://repos.wowace.com/wow/libdbicon-1-0/trunk
    tag: latest
  EnhanceQoL/libs/LibDataBroker-1.1: 
    url: https://github.com/tekkub/libdatabroker-1-1
    tag: latest

enable-nolib-creation: no

ignore:
  - scripts
  - .vscode
  - generate_changelog.sh
  - README.md
  - CONTRIBUTING.md

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown

move-folders:
  EnhanceQoL/EnhanceQoL: EnhanceQoL
  EnhanceQoL/EnhanceQoLDrinkMacro: EnhanceQoLDrinkMacro
  EnhanceQoL/EnhanceQoLMythicPlus: EnhanceQoLMythicPlus
  EnhanceQoL/EnhanceQoLTooltip: EnhanceQoLTooltip